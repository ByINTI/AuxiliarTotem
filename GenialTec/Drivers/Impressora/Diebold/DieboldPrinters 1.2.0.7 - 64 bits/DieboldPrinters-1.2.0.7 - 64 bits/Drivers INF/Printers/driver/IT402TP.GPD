*GPDSpecVersion: "1.0"
*Include: "StdNames.gpd"
*GPDFileVersion: "1.1"
*GPDFileName: "IT402TP.GPD"
*ModelName: "Diebold Procomp IM4X2T"
*MasterUnits: PAIR(203, 203)
*ResourceDLL: "ME_DRV_T230A.Dll"
*PrinterType: SERIAL
*Feature: Orientation
{
    *rcNameID: =ORIENTATION_DISPLAY
    *DefaultOption: PORTRAIT
    *Option: PORTRAIT
    {
        *rcNameID: =PORTRAIT_DISPLAY
    }
    *Option: LANDSCAPE_CC270
    {
        *rcNameID: =LANDSCAPE_DISPLAY
    }
}
*Feature: InputBin
{
    *rcNameID: =PAPER_SOURCE_DISPLAY
    *DefaultOption: Option1
    *Option: Option1
    {
        *rcNameID: 259
        *OptionID: 259
    }
}
*Feature: Resolution
{
    *rcNameID: =RESOLUTION_DISPLAY
    *DefaultOption: Option1
    *Option: Option1
    {
        *Name: "203 x 203 " =DOTS_PER_INCH
        *DPI: PAIR(203, 203)
        *TextDPI: PAIR(203, 203)
        *MinStripBlankPixels: 50
        *SpotDiameter: 100
        *Command: CmdSendBlockData { *Cmd : "<1B>J" %c[0,50]{max_repeat(NumOfDataBytes )} }
        *Command: CmdEndBlockData { *Cmd : "<0A>" }
    }
}
*Feature: PaperSize
{
    *rcNameID: =PAPER_SIZE_DISPLAY
    *DefaultOption: Option7
    *Option: Option1
    {
        *rcNameID: 268
        *OptionID: 268
        *PageDimensions: PAIR(456, 800)
        *PrintableArea: PAIR(400, 800)
        *PrintableOrigin: PAIR(28, 0)
    }
    *Option: Option2
    {
        *rcNameID: 269
        *OptionID: 269
        *PageDimensions: PAIR(456, 1199)
        *PrintableArea: PAIR(400, 1199)
        *PrintableOrigin: PAIR(28, 0)
    }
    *Option: Option3
    {
        *rcNameID: 270
        *OptionID: 270
        *PageDimensions: PAIR(456, 1599)
        *PrintableArea: PAIR(400, 1599)
        *PrintableOrigin: PAIR(28, 0)
    }
    *Option: Option4
    {
        *rcNameID: 271
        *OptionID: 271
        *PageDimensions: PAIR(456, 1998)
        *PrintableArea: PAIR(400, 1998)
        *PrintableOrigin: PAIR(28, 0)
    }
    *Option: Option5
    {
        *rcNameID: 272
        *OptionID: 272
        *PageDimensions: PAIR(456, 2398)
        *PrintableArea: PAIR(400, 2398)
        *PrintableOrigin: PAIR(28, 0)
    }
    *Option: Option6
    {
        *rcNameID: 274
        *OptionID: 274
        *PageDimensions: PAIR(456, 3996)
        *PrintableArea: PAIR(400, 3996)
        *PrintableOrigin: PAIR(28, 0)
    }
    *Option: Option7
    {
        *rcNameID: 275
        *OptionID: 275
        *PageDimensions: PAIR(456, 23976)
        *PrintableArea: PAIR(400, 23976)
        *PrintableOrigin: PAIR(28, 0)
    }
}
*Feature: Halftone
{
    *rcNameID: =HALFTONING_DISPLAY
    *DefaultOption: HT_PATSIZE_AUTO
    *Option: HT_PATSIZE_AUTO
    {
        *rcNameID: =HT_AUTO_SELECT_DISPLAY
    }
    *Option: HT_PATSIZE_SUPERCELL_M
    {
        *rcNameID: =HT_SUPERCELL_DISPLAY
    }
    *Option: HT_PATSIZE_6x6_M
    {
        *rcNameID: =HT_DITHER6X6_DISPLAY
    }
    *Option: HT_PATSIZE_8x8_M
    {
        *rcNameID: =HT_DITHER8X8_DISPLAY
    }
}

*Feature: Corte
{
    *Name: "Corte"
    *FeatureType: PRINTER_PROPERTY
    *DefaultOption: CorteParcial
    *Option: CorteParcial
    {
        *Name: "Parcial"
        *Command: CmdEndJob
         {
             *Order: JOB_FINISH.1
             *Cmd: "<10>"
         }
    }
    *Option: Total
    {
        *Name: "Total (Consulte Manual)"
        *Command: CmdEndJob
        {
            *Order: JOB_FINISH.1
            *Cmd: "<11>"
        }
    }
    *Option: Nenhum
    {
        *Name: "Sem corte"
    }
}


*Command: CmdStartDoc
{
    *Order: DOC_SETUP.1
    *Cmd: "<1B>G"
}
*%Command: CmdEndJob
*%{
*%    *Order: JOB_FINISH.1
*%    *Cmd: "<10>"
*%}
*RotateCoordinate?: FALSE
*RotateRaster?: FALSE
*RotateFont?: FALSE
*switch: Orientation
{
    *case: PORTRAIT
    {
        *TextCaps: LIST(TC_OP_CHARACTER,TC_EA_DOUBLE)
    }
    *case: LANDSCAPE_CC270
    {
        *TextCaps: LIST()
    }
}
*CursorXAfterCR: AT_CURSOR_X_ORIGIN
*CursorYAfterSendBlockData: AUTO_INCREMENT
*YMoveThreshold: *
*YMoveUnit: 203
*LineSpacingMoveUnit: 203
*Command: CmdSetLineSpacing { *Cmd: "<1B>H" %c[18,255]{LinefeedSpacing } }
*Command: CmdYMoveRelDown
{
    *CallbackID : 1
    *Params: LIST(DestYRel)
}
*Command: CmdLF { *Cmd : "<0A>" }
*Command: CmdFF { *Cmd : "<00>" }
*Command: CmdCR { *Cmd : "<00>" }
*EjectPageWithFF?: FALSE
*OutputDataFormat: H_BYTE
*OptimizeLeftBound?: FALSE
*CursorXAfterSendBlockData: AT_GRXDATA_END
*DefaultFont: 1
*DefaultCTT: 1
*switch: Orientation
{
    *case: PORTRAIT
    {
        *DeviceFonts: LIST(1,2,3,4)
    }
    *case: LANDSCAPE_CC270
    {
        *DeviceFonts: LIST()
    }
}
*Command: CmdBoldOn { *Cmd : "<1B>E" }
*Command: CmdBoldOff { *Cmd : "<1B>F" }
